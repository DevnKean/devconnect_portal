{% block WorkFromHomeType_row %}
    <div class="box box-info">
        <div class="box-header with-border">
            <div class="box-tools pull-right">
                <button type="button" class="collection-remove btn btn-danger btn-sm" title="Delete element"><i class="fa fa-trash"></i></button>
                <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i>
                </button>
            </div>
        </div>

        <!-- /.box-header -->
        <!-- form start -->
        <div class="box-body">
            <div class="col-sm-12">
                <div class="row">
                    <div class="col-sm-6">
                        {{ form_row(form.country) }}
                    </div>
                    <div class="col-sm-6">
                        {{ form_row(form.fte) }}
                    </div>

                </div>

            </div>
        </div>
    </div>
{% endblock %}

